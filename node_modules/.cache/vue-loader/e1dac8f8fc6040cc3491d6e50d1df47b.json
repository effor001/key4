olor:$font-color;\n    background-image:url(\"assets/digital_kairo2.svg\");\n    background-color:rgba(255,255,255,0.8);\n    background-blend-mode:lighten;\n    background-size:cover;\n    background-repeat:no-repeat;\n    overflow:hidden;\n  }\n  html, body {\n    height: calc(100% - 50px);\n  }\n  p{\n    font-size:1.25rem;\n    line-height: 2.4;\n  }\n\n\n  #app{\n    background: #fff;\n    width: 50%;\n    padding:5px;\n    border-radius: 10px;\n    height: calc(60% -50px);\n  }\n\n  .page{\n    position: fixed;\n    width: $width-wrap;\n  }\n\n  .base-anim-enter-active{\n    animation: coming 1s;\n    animation-delay: .5s;\n    opacity: 0;\n  }\n  .base-anim-leave-active{\n    animation: going 1s;\n  }\n\n  @keyframes going{\n    from{\n      transform: translateX(0);\n    }\n    to{\n      transform: translateX(-50px);\n      opacity:0;\n    }\n  }\n\n  @keyframes coming{\n    from{\n      transform: translateX(-50px);\n      opacity: 0;\n    }\n    to{\n      transform: translateX(0px);\n      opacity: 1;\n    }\n}\n// コンテンツエリア\n  .main{\n    width: $main-wrap;\n  }\n  .side{\n    width: $side-wrap;\n    writing-mode: vertical-lr;\n  }\n// ボタン\n  .btn{\n    display: inline-block;\n    width:300px;\n    max-width: 100%;\n    padding:20px 10px;\n    background-color: #fff;\n    border:2px solid #383736;\n    color:$font-color;\n    text-align:center;\n    text-decoration:none;\n    transition:.25s;\n\n   &:focus,&:hover{\n     background-color: #fff;\n     border-color:$font-color;\n     color:$hover-color;\n   }\n }\n// コピーライト\n .copy{\n   writing-mode:vertical-lr;\n   margin-top:10em;\n   position:fixed;\n\n}\n</style>\n"]}]}