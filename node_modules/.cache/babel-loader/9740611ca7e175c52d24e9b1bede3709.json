HA,CAAA;;AAKA,aAAA,MAAA;AACA;AAxBA;AAxBA,CAAA","sourcesContent":["<template>\n    <div class=\"MouseCursor\" v-on:mousemove=\"setNewCords\">\n        <div class=\"MouseCursorElement\" v-bind:style=\"getCurrentStyles()\" v-bind:class=\"{isAnimated: isAnimated}\"></div>\n        <slot></slot>\n    </div>\n</template>\n\n<script>\n    import '@/sass/MouseCursor.css';\n    export default {\n        name: 'MouseCursor',\n        props: {\n            radius: {\n                type: Number,\n                default: 30,\n            },\n            hoverRadius: {\n                type: Number,\n                default: 60\n            },\n            isHover: {\n                type: Boolean,\n                default: false\n            },\n            isAnimated: {\n                type: Boolean,\n                default: true\n            }\n        },\n        data: () => ({\n            x: 0,\n            y: 0,\n        }),\n        methods: {\n            setNewCords: function (event) {\n                this.x = event.pageX;\n                this.y = event.pageY;\n            },\n            getCurrentRadius: function () {\n                if (this.isHover !== true)\n                    return this.radius;\n                return this.hoverRadius;\n            },\n            calcCords: function () {\n                return {\n                    top: (this.y - (this.getCurrentRadius() / 2)) + 'px',\n                    left: (this.x - (this.getCurrentRadius() / 2)) + 'px',\n                }\n            },\n            getCurrentStyles: function () {\n                const radius = this.getCurrentRadius() + 'px';\n                const styled = {\n                    width: radius,\n                    height: radius,\n                    ...this.calcCords()\n                };\n                return styled;\n            }\n        }\n    };\n</script>\n"],"sourceRoot":"src/components"}]}